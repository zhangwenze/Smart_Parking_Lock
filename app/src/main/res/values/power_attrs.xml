<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PowerCharts">
        <attr name="radius" format="dimension"></attr>
        <attr name="strokeWidth" format="dimension"></attr>
        <attr name="circleColor" format="color"></attr>
        <attr name="ringColor_full" format="color"></attr><!-- 电量充足的颜色-->
        <attr name="ringColor_middle" format="color"></attr><!--电量中等的颜色-->
        <attr name="ringColor_alert" format="color"></attr><!--电量低的颜色-->
    </declare-styleable>

    <declare-styleable name="PullScrollView">
        <attr name="header" format="reference" />
        <attr name="headerHeight" format="dimension" />
        <attr name="headerVisibleHeight" format="dimension" />
    </declare-styleable>
</resources>