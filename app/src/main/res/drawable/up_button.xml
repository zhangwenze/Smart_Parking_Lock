<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape android:shape="oval">
            <stroke android:color="#ffff" android:width="1dp"></stroke>
            <solid android:color="#ff8033"></solid>
            <corners android:radius="360dp"></corners>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke android:color="#ff8033" android:width="1dp"></stroke>
            <solid android:color="#ff8033"></solid>
            <corners android:radius="360dp"></corners>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:color="#cc5500" android:width="1dp"></stroke>
            <solid android:color="#cc5500"></solid>
            <corners android:radius="360dp"></corners>
        </shape>
    </item>
</selector>