<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:state_focused="true">
            <shape android:shape="oval">
                <stroke android:color="#ffff" android:width="1dp"></stroke>
                <solid android:color="#8CE600"></solid>
                <corners android:radius="360dp"></corners>
            </shape>
        </item>
        <item android:state_pressed="false">
            <shape android:shape="oval">
                <stroke android:color="#8CE600" android:width="1dp"></stroke>
                <solid android:color="#8CE600"></solid>
                <corners android:radius="360dp"></corners>
            </shape>
        </item>
        <item android:state_pressed="true">
            <shape android:shape="oval">
                <stroke android:color="#66FF59" android:width="1dp"></stroke>
                <solid android:color="#66FF59"></solid>
                <corners android:radius="360dp"></corners>
            </shape>
        </item>
</selector>